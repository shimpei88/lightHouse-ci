{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "startServerCommand": "yarn start",
      "url": [
        "http://localhost:3000",
        "http://localhost:3000/about"
      ]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
